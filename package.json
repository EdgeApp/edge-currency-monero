{
  "name": "edge-currency-monero",
  "version": "1.0.0",
  "description": "Edge Monero currency plugin",
  "homepage": "https://edge.app",
  "repository": {
    "type": "git",
    "url": "git@github.com:EdgeApp/edge-currency-monero.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "Edge (Airbitz, Inc)",
  "contributors": [
    "Paul Puey <paul@edge.app>"
  ],
  "main": "./index.js",
  "module": "./lib/index.js",
  "files": [
    "CHANGELOG.md",
    "index.js",
    "lib/*",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf lib && sucrase ./src -q -d ./lib -t flow && webpack",
    "fix": "npm run lint -- --fix && yarn-deduplicate",
    "flow": "flow",
    "lint": "eslint .",
    "precommit": "lint-staged && flow && npm test && npm run build",
    "prepare": "husky install && npm run build",
    "test": "nyc mocha -r sucrase/register -n no-experimental-fetch './test/**/*.js'",
    "posttest": "nyc report --reporter=html",
    "updot": "updot"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "dependencies": {
    "@mymonero/mymonero-bigint": "^1.4.2",
    "@mymonero/mymonero-money-format": "^1.4.2",
    "@mymonero/mymonero-nettype": "^1.4.2",
    "biggystring": "^4.1.3",
    "buffer": "^5.0.6",
    "uri-js": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@mymonero/mymonero-monero-client": "^2.1.20",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.1.2",
    "edge-core-js": "^0.19.48",
    "eslint": "^7.14.0",
    "eslint-config-standard-kit": "0.15.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "flow-bin": "^0.89.0",
    "husky": "^7.0.0",
    "lint-staged": "^10.5.3",
    "mocha": "^10.0.0",
    "node-fetch": "^2.1.2",
    "nyc": "^11.3.0",
    "prettier": "^2.2.0",
    "react-native-mymonero-core": "^0.3.0",
    "rimraf": "^2.6.2",
    "sucrase": "^3.9.5",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "yarn-deduplicate": "^6.0.1"
  },
  "peerDependencies": {
    "react-native-mymonero-core": "^0.2.7"
  },
  "react-native": "./lib/xmrIndex.js"
}
